{
  "name": "jsx-extractor",
  "displayName": "jsx-extractor",
  "publisher": "lesaaly",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.102.0"
  },
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:extractor.extractComponent"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extractor.extractComponent",
        "title": "Extract Component"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lesaaly/vscode-jsx-extractor"
  },
  "bugs": {
    "url": "https://github.com/lesaaly/vscode-jsx-extractor/issues"
  },
  "homepage": "https://github.com/lesaaly/vscode-jsx-extractor#readme",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "esbuild": "^0.25.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
